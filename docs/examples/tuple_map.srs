fn three_tuple_map{A, B}(x: (A, A, A), f: A->B): (B, B, B) {
    return (f(x[0]), f(x[1]), f(x[2]));
}

fn map_test_1(x: f64): bool {
    return x > 0;
}

fn map_test_2(x: f64): i64 {
    if x > 0 {
        return 1;
    } else {
        return -1;
    }
}

fn main() {
    let a = (1., -1., -0.34);
    print three_tuple_map(a, map_test_1);
    print three_tuple_map(a, map_test_2);
}